      "source": [
        "# https://www.acmicpc.net/problem/11727\n",
        "\n",
        "n = int(input())\n",
        "\n",
        "dp = [0, 1, 3]\n",
        "\n",
        "for i in range(3, n+1):\n",
        "    dp.append((dp[i-1]) + ((dp[i-2]) * 2))\n",
        "\n",
        "print(dp[n] % 10007)"
      ]
    }
